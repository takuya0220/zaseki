{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {
        "js": "gulp | npm start"
    },
    "events": {
        "restart": ""
    },
    "watch": [
        "bin/",
        "public/",
        "routes/",
        "views/",
        "resources"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js,json,jade"
}
