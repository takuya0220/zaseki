{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "npm start"
  },
  "events": {
    "restart": ""
  },
  "watch": [
    "bin/",
    "public/",
    "routes/",
    "views/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json,jade"
}
